{
  "name": "great-reads",
  "version": "1.0.0",
  "description": "AMEX Code Challenge",
  "author": "Amy De Genaro",
  "license": "ISC",
  "main": "index.js",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "build-watch": "webpack -w",
    "bundle-add": "git checkout -b deploy && git add -A -f public/bundle.js public/bundle.js.map",
    "bundle-cm": "git cm 'Deploying'",
    "cleanup": "git checkout main && git branch -D deploy",
    "deploy": "npm run bundle-add && npm run bundle-cm && npm run heroku && npm run cleanup",
    "heroku": "git push -f heroku main",
    "lint": "eslint ./client ./server",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-watch & npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amydegenaro/great-reads.git"
  },
  "bugs": {
    "url": "https://github.com/amydegenaro/great-reads/issues"
  },
  "homepage": "https://github.com/amydegenaro/great-reads#readme",
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.5.3",
    "axios": "^0.26.1",
    "express": "^4.17.3",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.3.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.4",
    "nodemon": "^2.0.15",
    "redux-logger": "^3.0.6",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2"
  }
}
